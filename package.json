{
  "name": "d-rts",
  "version": "1.0.0",
  "description": "React component library with Tailwind CSS, shadcn/ui patterns, and Radix UI primitives",
  "type": "module",
  "author": "Rogerio Piveto <pivetorogerio@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pivetoo/d-tailwind-shadcn-lib.git"
  },
  "bugs": {
    "url": "https://github.com/pivetoo/d-tailwind-shadcn-lib/issues"
  },
  "homepage": "https://github.com/pivetoo/d-tailwind-shadcn-lib#readme",
  "keywords": [
    "react",
    "components",
    "ui",
    "tailwind",
    "shadcn",
    "radix-ui"
  ],
  "main": "./d-rts/dist/index.umd.cjs",
  "module": "./d-rts/dist/index.js",
  "types": "./d-rts/dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./d-rts/dist/index.d.ts",
      "import": "./d-rts/dist/index.js",
      "require": "./d-rts/dist/index.umd.cjs"
    },
    "./styles": "./d-rts/dist/style.css"
  },
  "files": [
    "d-rts/dist"
  ],
  "scripts": {
    "dev": "cd d-rts && npm run dev",
    "build": "cd d-rts && npm install && npm run build"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "axios": "^1.13.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.552.0",
    "tailwind-merge": "^3.3.1"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@tailwindcss/postcss": "^4.1.16",
    "@types/node": "^24.9.2",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "postcss": "^8.5.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwindcss": "^3.4.18",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7",
    "vite-plugin-dts": "^4.5.4"
  }
}
